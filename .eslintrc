{
  "extends": [
    "airbnb",
    "airbnb/hooks",
    "prettier/react",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "jsx-a11y/label-has-associated-control": [
      2,
      {
        "labelComponents": ["<span>"]
      }
    ]
  }
}
